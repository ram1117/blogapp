  <%= link_to user_post_path(post.author.id,post.id) do%>
  
  <div class="post-tile">
  <h3>Post #: <%= post.id %> <%=post.title %></h3>
  <h4>Author: <%= post.author.name%></h4>
  <p><%= post.text%></p>
  <%= link_to user_post_likes_path(user_id: post.author.id,post_id: post.id), data:{turbo_method: :post} do %>
  <button class="likes-btn">Like</button>
  <%= button_to "delete post",user_post_path(user_id: post.author.id,post_id:post.id), data:{turbo_method: :delete},class: 'post-delete-btn' %>
  <%end%>
  <p class="comments-likes" >Comments: <%= post.comments_counter%> Likes: <%= post.likes_counter%> </p>
  </div>
  <%end%>